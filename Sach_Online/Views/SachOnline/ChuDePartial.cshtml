@model IEnumerable<Sach_Online.Models.CHUDE>
<h4 style="text-align:center">Sách theo Chủ Đề</h4>
<ul class="list-group">
    <li class="list-group-item active" style="font-weight:bold; text-align:center">


        @foreach (var item in Model)
        {

            <a href=@Url.Action("SachTheoChuDe","SachOnline", new { id = item.MaCD }) class="list-group-item ">@item.TenChuDe</a>
        }
</ul>

